-- Scripts:

local function FPBEFD_fake_script() -- FpsDisplay.Script 
	local script = Instance.new('Script', FpsDisplay)

	local RS = game:GetService("RunService")
	local frames = 0
	
	RS.RenderStepped:Connect(function()
		frames = frames + 1
	end)
	
	while wait(1) do
		script.Parent.Text = frames .. " Fps"
		frames = 0
	end
end
coroutine.wrap(FPBEFD_fake_script)()
local function NLED_fake_script() -- DestroyButton.DestroyScript 
	local script = Instance.new('Script', DestroyButton)

	local Button = script.Parent
	
	Button.MouseButton1Click:Connect(function()
		script.Parent.Parent.Parent:Destroy() 
	end)
end
coroutine.wrap(NLED_fake_script)()
local function PJSI_fake_script() -- Regone.DragScript 
	local script = Instance.new('Script', Regone)

	local UserInputService = game:GetService("UserInputService")
	
	local gui = script.Parent
	
	local dragging
	local dragInput
	local dragStart
	local startPos
	
	local function update(input)
		local delta = input.Position - dragStart
		gui.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
	end
	
	gui.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			dragging = true
			dragStart = input.Position
			startPos = gui.Position
	
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)
	
	gui.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			dragInput = input
		end
	end)
	
	UserInputService.InputChanged:Connect(function(input)
		if input == dragInput and dragging then
			update(input)
		end
	end)
end
coroutine.wrap(PJSI_fake_script)()
local function EQPGXPD_fake_script() -- AutoFarm.AutoFarmScript 
	local script = Instance.new('Script', AutoFarm)

	local Button = script.Parent
	
	Button.MouseButton1Click:connect(function()
		local groundDistance = 15
		local Player = game:GetService("Players").LocalPlayer
		local function getNearest()
			local nearest, dist = nil, 99999
			for _,v in pairs(game.Workspace.BossFolder:GetChildren()) do
				if(v:FindFirstChild("Head")~=nil)then
					local m =(Player.Character.Head.Position-v.Head.Position).magnitude
					if(m<dist)then
						dist = m
						nearest = v
					end
				end
			end
			for _,v in pairs(game.Workspace.enemies:GetChildren()) do
				if(v:FindFirstChild("Head")~=nil)then
					local m =(Player.Character.Head.Position-v.Head.Position).magnitude
					if(m<dist)then
						dist = m
						nearest = v
					end
				end
			end
			return nearest
		end
		_G.farm2 = true
		Player.Chatted:Connect(function(m)
			if(m==";autofarm false")then
				_G.farm2 = false
			elseif(m==";autofarm true")then
				_G.farm2 = true
			end
		end)
		_G.globalTarget = nil
		game:GetService("RunService").RenderStepped:Connect(function()
			if(_G.farm2==true)then
				local target = getNearest()
				if(target~=nil)then
					game:GetService("Workspace").CurrentCamera.CFrame = CFrame.new(game:GetService("Workspace").CurrentCamera.CFrame.p, target.Head.Position)
					Player.Character.HumanoidRootPart.CFrame = (target.HumanoidRootPart.CFrame * CFrame.new(0, groundDistance, 15))
					_G.globalTarget = target
				end
			end
		end)
		spawn(function()
			while wait() do
				game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(0,0,0)
				game.Players.LocalPlayer.Character.Torso.Velocity = Vector3.new(0,0,0)
			end
		end)
		while wait() do
			if(_G.farm2==true and _G.globalTarget~=nil and _G.globalTarget:FindFirstChild("Head") and Player.Character:FindFirstChildOfClass("Tool"))then
				local target = _G.globalTarget
				game.ReplicatedStorage.Gun:FireServer({["Normal"] = Vector3.new(0, 0, 0), ["Direction"] = target.Head.Position, ["Name"] = Player.Character:FindFirstChildOfClass("Tool").Name, ["Hit"] = target.Head, ["Origin"] = target.Head.Position, ["Pos"] = target.Head.Position,})
				wait()
			end
		end
	end)
end
coroutine.wrap(EQPGXPD_fake_script)()
local function NTDNNRZ_fake_script() -- SpinBot.Spinner 
	local script = Instance.new('Script', SpinBot)

	local switchActivated = false -- Initially, the switch is deactivated
	
	function Spina()
		if not switchActivated then
			switchActivated = true
			script.Parent.Text = "SpinBot ON" -- Change the button text to indicate it's now a switch off button
			--makes player beable to use shiftylock
			game:GetService("Players").LocalPlayer.Character.Humanoid.AutoRotate = false
			-- Execute your function
			power = 900 -- change this to make it more or less powerful
			wait(.1)
			local Spinner = Instance.new("BodyThrust")
			Spinner.Parent = game.Players.LocalPlayer.Character.HumanoidRootPart
			Spinner.Force = Vector3.new(power,0,power)
			Spinner.Location = game.Players.LocalPlayer.Character.HumanoidRootPart.Position
	
		else
			switchActivated = false
			script.Parent.Text = "SpinBot OFF" -- Change the button text to indicate it's now a switch on button
			-- Deactivate the function (you might want to remove the spinning)
	
			game:GetService("Players").LocalPlayer.Character.Humanoid.AutoRotate = true
			-- Example: stop all BodyThrust instances
			for _, bodyThrust in ipairs(game.Players.LocalPlayer.Character.HumanoidRootPart:GetChildren()) do
				if bodyThrust:IsA("BodyThrust") then
					bodyThrust:Destroy()
				end
			end
		end
	end
	
	script.Parent.MouseButton1Click:Connect(Spina)
	
end
coroutine.wrap(NTDNNRZ_fake_script)()
local function LUWLOCG_fake_script() -- AntiAFK.AntiAFK 
	local script = Instance.new('Script', AntiAFK)

	-- Get the player's character
	local player = game.Players.LocalPlayer
	local character = player.Character or player.CharacterAdded:Wait()
	
	-- Function to send input to prevent AFK kick
	local function sendInput()
		local virtualUser = game:GetService("VirtualUser")
		virtualUser:Button2Down(Vector2.new())
		wait(1)
		virtualUser:Button2Up(Vector2.new())
	end
	
	-- Connect a function to the CharacterAdded event to get the new character
	player.CharacterAdded:Connect(function(newCharacter)
		character = newCharacter
	end)
	
	-- Loop to continuously send input
	while true do
		sendInput()
		wait(300) -- Wait 5 minutes before sending the next input
	end
	
end
coroutine.wrap(LUWLOCG_fake_script)()
local function RUIEHDW_fake_script() -- IteamChecker.AutoGun 
	local script = Instance.new('Script', IteamChecker)

	local player = game.Players.LocalPlayer
	local backpack = player.Backpack
	
	local function deleteOmegaScytheAndEquipOther()
		-- Find the "Omega Scythe" tool in the backpack
		local omegaScythe
		for _, tool in ipairs(backpack:GetChildren()) do
			if tool:IsA("Tool") and tool.Name == "Omega Scythe" then
				omegaScythe = tool
				break
			end
		end
	
		-- If the "Omega Scythe" is found, delete it
		if omegaScythe then
			omegaScythe:Destroy()
		end
	
		-- Find the first non-"Omega Scythe" tool in the backpack
		local toolToEquip
		for _, tool in ipairs(backpack:GetChildren()) do
			if tool:IsA("Tool") and tool.Name ~= "Omega Scythe" then
				toolToEquip = tool
				break
			end
		end
	
		-- If a non-"Omega Scythe" tool is found, equip it
		if toolToEquip then
			player.Character.Humanoid:EquipTool(toolToEquip)
		end
	end
	
	-- Call the function every 3 minutes
	while true do
		deleteOmegaScytheAndEquipOther()
		wait(120) -- 3 minutes in seconds
	end
	
end
coroutine.wrap(RUIEHDW_fake_script)()
local function ZYUZGUE_fake_script() -- OptiButton.Optimizer 
	local script = Instance.new('Script', OptiButton)

	local opti = script.Parent
	
	opti.MouseButton1Click:Connect(function()
		local decalsyeeted = true 
		local g = game
		local w = g.Workspace
		local l = g.Lighting
		local t = w.Terrain
		t.WaterWaveSize = 0
		t.WaterWaveSpeed = 0
		t.WaterReflectance = 0
		t.WaterTransparency = 0
		l.GlobalShadows = false
		l.FogEnd = 9e9
		l.Brightness = 0
		settings().Rendering.QualityLevel = "Level01"
		for i,v in pairs(g:GetDescendants()) do
			if v:IsA("Part") or v:IsA("Union") or v:IsA("MeshPart") then
				v.Material = "Plastic"
				v.Reflectance = 0
			elseif v:IsA("Decal") and decalsyeeted then 
				v.Transparency = 1
			elseif v:IsA("ParticleEmitter") or v:IsA("Trail") then 
				v.Lifetime = NumberRange.new(0)
			end
		end
	end)
end
coroutine.wrap(ZYUZGUE_fake_script)()
local function LPWTXFK_fake_script() -- Regone.Keybind 
	local script = Instance.new('Script', Regone)

	-- Get the UI frame you want to toggle
	local player = game.Players.LocalPlayer
	local frame = player.PlayerGui.RegoneMain.Regone
	
	-- Create a boolean variable to track the visibility state
	local isVisible = true
	
	-- Connect a function to the UserInputService's InputBegan event
	game:GetService("UserInputService").InputBegan:Connect(function(input, gameProcessedEvent)
		-- Check if the input is the "V" key and the game didn't process the input
		if input.KeyCode == Enum.KeyCode.V and not gameProcessedEvent then
			-- Toggle the visibility state
			isVisible = not isVisible
	
			-- Set the frame's visibility based on the new state
			frame.Visible = isVisible
		end
	end)
	
end
coroutine.wrap(LPWTXFK_fake_script)()
