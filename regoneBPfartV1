-- Scripts:

local function OYWLORZ_fake_script() -- antlerCase.Antler script 
	local script = Instance.new('Script', antlerCase)

	local lastPressed = 0
	local cooldown = 30
	
	local function Ribbon()
		local currentTime = tick()
		if currentTime - lastPressed >= cooldown then
			lastPressed = currentTime
			local args = {
				[1] = 66,
				[2] = "Accessories",
				[3] = "Knife Antlers"
			}
			game:GetService("ReplicatedStorage").RemoteFunction:InvokeServer(unpack(args))
		end
	end
	
	script.Parent.MouseButton1Click:Connect(Ribbon)
end
coroutine.wrap(OYWLORZ_fake_script)()
local function CEGDFLH_fake_script() -- KnifeCase.KnifeScript 
	local script = Instance.new('Script', KnifeCase)

	local lastPressed = 0
