			lastPressed = currentTime
			local args = {
				[1] = 66,
				[2] = "Knife Skins",
				[3] = "Winter Gift"
			}
			game:GetService("ReplicatedStorage").RemoteFunction:InvokeServer(unpack(args))
		end
	end
	
	script.Parent.MouseButton1Click:Connect(Knife)
	
end
coroutine.wrap(CEGDFLH_fake_script)()
